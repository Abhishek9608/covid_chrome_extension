{
  "name": "Covid Statictics",
  "version": "1.0",
  "description": "this will give you the current status of covid Impact",
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' https://apis.google.com; object-src 'self'",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["js/popup.js"]
    }
  ],
  "browser_action": {
    "default_popup": "html/popup1.html",
    "default_title": "covid"
  }
}
